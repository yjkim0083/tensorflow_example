    "import tensorflow as tf\n",
    "\n",
    "a = tf.constant([5],dtype=tf.float32)\n",
    "b = tf.constant([10],dtype=tf.float32)\n",
    "c = tf.constant([2],dtype=tf.float32)\n",
    "\n",
    "d = a * b + c\n",
    "\n",
    "sess = tf.Session()\n",
    "result = sess.run(d)\n",
    "\n",
    "print result\n",
    "\n",
    "sess.close()"
   ]
  }
